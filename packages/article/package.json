{
  "name": "@times-components/article",
  "version": "6.4.5",
  "description": "The article",
  "main": "dist/article",
  "dev": "src/article",
  "scripts": {
    "fmt": "eslint . --fix && prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "depcheck": "depcheck --ignores='@thetimes/jest-lint,babel-*,depcheck,eslint,jest,prettier,react-art,stylelint*,webpack*' --ignore-bin-package=false --skip-missing",
    "lint": "eslint . && yarn prettier:diff && yarn depcheck && jest-lint",
    "prepublishOnly": "yarn transpile && yarn bundle",
    "test": "jest --config='./__tests__/jest.config.js'",
    "cleanup-dist": "rm -rf dist",
    "transpile": "yarn cleanup-dist && babel src -d dist",
    "bundle": "NODE_ENV=production webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "keywords": [
    "react-native-web",
    "react",
    "native",
    "web",
    "article",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@thetimes/jest-lint": "*",
    "@times-components/eslint-config-thetimes": "0.8.8",
    "@times-components/jest-configurator": "2.1.17",
    "@times-components/provider-test-tools": "1.10.3",
    "@times-components/storybook": "3.1.26",
    "@times-components/tealium-utils": "0.7.8",
    "@times-components/webpack-configurator": "2.0.14",
    "babel-core": "^7.0.0-bridge",
    "babel-jest": "^23.4.2",
    "babel-loader": "8.0.4",
    "depcheck": "0.6.9",
    "eslint": "5.9.0",
    "jest": "23.6.0",
    "prettier": "1.14.3",
    "react": "16.5.2",
    "react-art": "16.5.2",
    "react-dom": "16.5.2",
    "react-native": "0.59.4",
    "react-native-web": "0.11.0",
    "react-test-renderer": "16.5.2",
    "stylelint": "9.4.0",
    "stylelint-config-recommended": "2.1.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.3.2",
    "webpack": "4.22.0",
    "webpack-cli": "3.1.2"
  },
  "dependencies": {
    "@times-components/ad": "2.2.2",
    "@times-components/article-magazine-comment": "0.1.4",
    "@times-components/article-main-comment": "0.3.3",
    "@times-components/article-main-standard": "0.3.2",
    "@times-components/context": "0.5.13",
    "@times-components/provider": "1.8.4",
    "@times-components/styleguide": "3.9.0",
    "lodash.invert": "4.3.0"
  },
  "peerDependencies": {
    "react": ">=16.5",
    "react-dom": ">=16.5",
    "react-native": ">=0.55",
    "react-native-web": "0.11.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
